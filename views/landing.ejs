    </head>
    <body>
    
    <div class="container">
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>
    
    <div id="landing-header">
		<h1 style = "padding-right:50px"><i class="fas fa-shoe-prints fa-rotate-270"></i> Footprint</h1>
		<a class="btn btn-secondary btn-lg" href="/gallery">Go to Gallery</a>
    </div>
    
    <ul class="slideshow">
